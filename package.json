{
  "name": "rebate_backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "schema_local": "psql -U postgres -d batistack_dev -f db/schema.sql",
